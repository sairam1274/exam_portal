<h1>Listing answers</h1>

<table>
  <tr>
    <th>User</th>
    <th>Technology</th>
    <th>Topic</th>
    <th>Is correct</th>
    <th>Question type</th>
    <th>Question</th>
    <th>Question title</th>
    <th></th>
    <th>Actual answer ids</th>
    <th></th>
    <th>Given answer ids</th>
    <th>Actual answers</th>
    <th>Given answers</th>
    <th>Free text answer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @answers.each do |answer| %>
  <tr>
    <td><%= answer.user_id %></td>
    <td><%= answer.technology_id %></td>
    <td><%= answer.topic_id %></td>
    <td><%= answer.is_correct %></td>
    <td><%= answer.question_type %></td>
    <td><%= answer.question_id %></td>
    <td><%= answer.question_title %></td>
    <td><%= answer. %></td>
    <td><%= answer.actual_answer_ids %></td>
    <td><%= answer. %></td>
    <td><%= answer.given_answer_ids %></td>
    <td><%= answer.actual_answers %></td>
    <td><%= answer.given_answers %></td>
    <td><%= answer.free_text_answer %></td>
    <td><%= link_to 'Show', answer %></td>
    <td><%= link_to 'Edit', edit_answer_path(answer) %></td>
    <td><%= link_to 'Destroy', answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Answer', new_answer_path %>
