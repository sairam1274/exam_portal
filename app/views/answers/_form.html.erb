<%= simple_form_for(@answer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :technology_id %>
    <%= f.input :topic_id %>
    <%= f.input :is_correct %>
    <%= f.input :question_type %>
    <%= f.input :question_id %>
    <%= f.input :question_title %>
    <%= f.input : %>
    <%= f.input :actual_answer_ids %>
    <%= f.input : %>
    <%= f.input :given_answer_ids %>
    <%= f.input :actual_answers %>
    <%= f.input :given_answers %>
    <%= f.input :free_text_answer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
