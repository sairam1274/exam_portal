<%= simple_form_for(@question) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :technology_id %>
    <%= f.input :topic_id %>
    <%= f.input :title %>
    <%= f.input :question_type %>
    <%= f.input :parent_id %>
    <%= f.input :note %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
